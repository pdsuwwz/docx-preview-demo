{
  "name": "docx-preview-demo",
  "version": "3.0",
  "description": "Docx Preview Demo",
  "scripts": {
    "start": "concurrently 'pnpm frontend' 'pnpm backend'",
    "frontend": "pnpm --filter \"frontend\" dev",
